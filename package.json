{
  "name": "@degen/common-utils",
  "version": "0.2.0",
  "description": "NPM package containing common utilities for various node projects.",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": "git@github.com:degen-workgroup/common-utils.git",
  "author": "Brian <patinobrian@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "build": "tsc -p tsconfig.json",
    "test": "jest --config jest.config.js",
    "prestart": "pnpm install && pnpm lint && pnpm build",
    "start": "node --trace-warnings dist/main.js",
    "prepublish": "pnpm prestart && pnpm test",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "~27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "jest": "~27.4.7",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@logdna/logger": "^2.6.1"
  },
  "files": [
    "dist/**/*"
  ]
}
